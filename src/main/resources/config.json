{
  # Auto-Reset : start | end | both | none
  "auto-reset": "start",

  # Cancel jump damage
  "cancel-jump-damage": true,

  # Automatically put column where player fell.
  "put-column": true,

  # Turn Time Out (ticks)
  "turn-time-out": 1200,

  # Tp before jump
  "tp-before-jump": true,

  # Tp after jump
  "tp-after-jump": true,

  # Tp delay after fail (ticks). Negative values mean deactivated.
  "tp-after-fail": 60,

  # Safe region Height / Margin
  # This is the region above the pool where the player can jump without dying.
  "safe-region-height": 5,
  "safe-region-margin": 5,

  # Indicates if messages should be prefixed with the arena's name
  # for spectator's messages.  "auto-reset": "start",
  "prefix-spectators-messages": true
}